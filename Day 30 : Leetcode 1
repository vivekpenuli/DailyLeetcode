// Rearrange Array Elements by Sign: //

class Solution {
    public int[] rearrangeArray(int[] nums) {
        int[] arr=new int[nums.length];
        int A1=0,A2=1;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]>0)
            {
                arr[A1]=nums[i];
            A1=A1+2;;
            }
            else
            {
                arr[A2]=nums[i];
                A2=A2+2;
            }

        }
        return arr;
    }
}

// Ouput Expexted
