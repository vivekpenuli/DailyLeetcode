// 1.Best Time to buy and sell stock //

class Solution {
    public int maxProfit(int[] prices) {
int maxprofit=0,newprofit=0;
int trading=prices[0];
for(int i=0;i<prices.length;i++)
{
if(trading>prices[i])
{
    trading=prices[i];
}
newprofit = prices[i]-trading;
if(maxprofit<newprofit)
{
    maxprofit=newprofit;
}


}
return maxprofit;
    }
}

// Expected Output


