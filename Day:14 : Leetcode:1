
// Find All Duplicates in an Array 
class Solution {
    public List<Integer> findDuplicates(int[] nums) {
       List<Integer> ls=new ArrayList<>();
       int index;
       for(int i=0;i<nums.length;i++)
       {
            index=Math.abs(nums[i])-1;
            if(nums[index]<0)
            ls.add(Math.abs(nums[i]));
            else
            nums[index]=-nums[index];
       }

       return ls; 
    }
}
------------------------------------------------------------
// Expected Output :
